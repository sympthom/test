{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
   
  },
  "variables": {
    "instanceName": "[replace(resourceGroup().name, 'sof-', '')]",
    "applicationInsightsName": "[concat(variables('instanceName'),'-insight-ai')]"
  },
  "resources": [
    {
      "apiVersion": "2018-05-01",
      "name": "[variables('applicationInsightsName')]",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(deployment().properties.templateLink.uri, 'template-applicationinsights.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "resourceName": { "value": "insight" }
        }
      },
      "dependsOn": [

      ]
    }
  ]
}